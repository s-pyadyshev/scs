    .video__overlay(style="background-image: url(./images/poster.jpg); background-size: cover;")
    iframe(src="https://player.vimeo.com/video/46926279" width="100%" height="564" frameborder="0" allow="autoplay; fullscreen; picture-in-picture" allowfullscreen)
    button.video__play.js-play-video
      +svg('play')