mixin videoVimeo(poster)
  .video(class!=attributes.class)
    .video__overlay
      img(src=poster alt="video poster.")
    iframe(src="https://player.vimeo.com/video/46926279" width="100" height="564" allow="autoplay; fullscreen; picture-in-picture" allowfullscreen)
    button.video__play.js-play-video
      +svg('play')